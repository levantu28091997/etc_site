





















/* =========================== Theme new css ====================== */


/*base*/
ul,li{padding: 0; margin: 0; list-style: none;}
.bold{font-weight: bold;}
.normal{font-weight: normal;}
.mincho{font-family: "FOT-筑紫Cオールド明朝 Pr6 R", "游明朝体", "Yu Mincho", YuMincho, "Hiragino Kaku Gothic ProN", "Hiragino Kaku Gothic Pro", "メイリオ", Meiryo, "ＭＳ ゴシック", sans-serif;}
.text-right{text-align: right;}
.text-left{text-align: left;}
.text-center{text-align: center;}
a.uline{text-decoration: underline;}
.short{-webkit-box-orient:vertical;display:-webkit-box;-webkit-line-clamp:1;overflow:hidden;}
.short.line-2{-webkit-line-clamp:2;}
.short.line-3{-webkit-line-clamp:3;}
.short-line{-webkit-box-orient:vertical;display:-webkit-box;-webkit-line-clamp:2;overflow:hidden;}

/*anime*/
.anime{transition: .3s;}
.opa0,.sc-anime,.sc-op,.op-anime{opacity: 0;}


/*margin*/
.mr0{margin-right: 0 !important;}
.mr5{margin-right: 5px !important;}
.mr10{margin-right: 10px !important;}
.mr15{margin-right: 15px !important;}
.mr20{margin-right: 20px !important;}
.ml5{margin-left: 5px !important;}
.ml10{margin-left: 10px !important;}
.mb5{margin-bottom: 5px !important;}
.mb0{margin-bottom: 0 !important;}
.mb10{margin-bottom: 10px !important;}
.mb20{margin-bottom: 20px !important;}
.mb30{margin-bottom: 30px !important;}
.mb40{margin-bottom: 40px !important;}
.mb50{margin-bottom: 50px !important;}
.mt0{margin-top: 0 !important;}
.mt5{margin-top: 5px !important;}
.mt10{margin-top: 10px !important;}
.mt20{margin-top: 20px !important;}
.mt30{margin-top: 30px !important;}
.mt40{margin-top: 40px !important;}
.mt50{margin-top: 50px !important;}

/*padding*/
.pt0{padding-top: 0 !important;}

/*width*/
.w-full{width: 100% !important; max-width: 100% !important;}
.w-100{width: 100px !important;}


/*font-size*/
.fsz-12{font-size: 12px !important;}
.fsz-14{font-size: 14px !important;}
.fsz-16{font-size: 16px !important;}
.fsz-18{font-size: 18px !important;}
.fsz-20{font-size: 20px !important;}
.fsz-24{font-size: 24px !important;}
.fsz-34{font-size: 34px !important;}

/*position*/
.ab{position: absolute;}
.rela{position: relative;}
.fa,.far,.fas{margin-right: 5px;}
.no-fa{margin-right: 0;}

/*hover*/
.hov-opa:hover{opacity: 0.8;}

/*map*/
.google-map {position: relative; width: 100%; height: 0; padding-bottom: 60%; overflow: hidden;}
.google-map iframe {position: absolute; top: 0; left: 0; width: 100%; height: 100%;}

/*flex*/
.fx-wrp{-webkit-flex-wrap:wrap; flex-wrap:wrap;}
.fx-wrp-rev{-webkit-flex-wrap:wrap-reverse; flex-wrap: wrap-reverse;}
.fx-aro{-webkit-justify-content:space-around; justify-content:space-around;}
.fx-coc{-webkit-justify-content:center; justify-content:center;}
.fx-ced{-webkit-justify-content:flex-end; justify-content:flex-end;}
.fx-str{-webkit-align-items:flex-start; align-items:flex-start;}
.fx-end{-webkit-align-items:flex-end; align-items:flex-end;}
.fx-itc{-webkit-align-items:center; align-items:center;}
.fx-cc{display: flex; align-items:center;}
.fx-bet{display: flex; -webkit-justify-content:space-between; justify-content:space-between;}
.fx-ccc{display: inline-flex; justify-content:center; align-items:center;}
.fx-ex{display: flex; -webkit-justify-content:space-between; justify-content:space-between; -webkit-align-items:center; align-items:center;}
.fx-cos{-webkit-justify-content:flex-start; justify-content:flex-start;}
.fx-arc{display: flex; -webkit-justify-content:space-between; justify-content:space-between; -webkit-flex-wrap:wrap; flex-wrap:wrap;}
.fx-rev{flex-direction: row-reverse;}
.fx-pc,.flex{display: -webkit-flex; display: flex;}

/*grid*/
.grid{display: grid; grid-gap: 20px;}
.grid-2{grid-template-columns: 1fr 1fr;}
.grid-3{grid-template-columns: 1fr 1fr 1fr;}
.grid-4{grid-template-columns: 1fr 1fr 1fr 1fr;}
.grid-5{grid-template-columns: 1fr 1fr 1fr 1fr 1fr;}

/*row*/
.row-5{margin: 0 -5px;}
.row-5 > div{padding: 0 5px;}
.row-7{margin: 0 -7px;}
.row-7 > div{padding: 0 7px;}
.row-10{margin: 0 -10px;}
.row-10 > div{padding: 0 10px;}
.row-12{margin: 0 -12px;}
.row-12 > div{padding: 0 12px;}

/*rotate*/
.r{display: inline-block;}
.r45{transform: rotate( 45deg );}
.r90{transform: rotate( 90deg );}
.r135{transform: rotate( 135deg );}
.r180{transform: rotate( 180deg );}
.r225{transform: rotate( 225deg );}
.r270{transform: rotate( 270deg );}
.r315{transform: rotate( 315deg );}
.r360{transform: rotate( 360deg );}

/*---PC---*/
@media (min-width: 768px){
    .pc-hidden{display: none;}
    
}

/*---スマホ---*/
@media only screen and (max-width: 767px) {
    .sm-hidden{display: none;}
    .fx-pc{display: block;}
}

img{max-width: 100%;}
a{text-decoration: none;}
a:hover{text-decoration: none;}
body{font-size: 16px; font-family: "游ゴシック体" , Yu Gothic, "Hiragino Kaku Gothic ProN", "Hiragino Kaku Gothic Pro", "ＭＳ ゴシック", sans-serif; color: #383838;}
span{display: block;}
.noto{font-family: 'Noto Serif JP', serif;}
.container{max-width: 1350px;}
.pad_block{display: none;}
section{overflow: hidden;}
.gray_bg{background-color: #F4F4F4;}

/*--- header ---*/
.head_left{width: 90%; padding: 15px 30px; background-color: #192049; color: #fff;}
.head_left .head_item a:nth-of-type(1){margin-right: 20px;}
.head_left .head_item a:nth-of-type(2){font-size: 18px;}
.head_left .head_item a:nth-of-type(1) i{margin-left: 10px;}
.head_left .head_item a{color: #fff;}
.head_right{width: 10%;}
.head_right a{display: block; width: 100%; height: 100%; text-align: center; position: relative; transition: .3s; background-color: #D1BA8A;}
.head_right a:hover{opacity: 0.6;}
.head_right a .wrp_text{display: block; position: absolute; width: 100%; height: 40px; top: 0; bottom: 0; left: 0; right: 0; margin: auto; color: #192049;}
.head_right a .sub{font-size: 10px;}
.head_bottom{background-color: #757991; padding: 15px 0;}
.head_bottom a{display: block; color: #fff; text-align: center; padding: 0 20px; margin: 0 30px; transition: .3s;}
.head_bottom a:hover{color: #D1BA8A;}
.head_logo h1{margin-bottom: 0;}
.head_logo h1 a span{color: #fff; font-size: 21px; letter-spacing: 2px;}
.head_logo h1 a img{max-width: 85px; margin-right: 10px;}

/*--- main ---*/
/* public */
main{padding-bottom: 120px;}
.section_inner{padding: 50px 0;}
.sec_title{margin-bottom: 40px; position: relative;}
.sec_title::before{content: ''; position: absolute; border-top: solid 1px #192049; top: 0; bottom: 0; left: -21.5%; height: 1px; width: 20%; margin: auto 0;}
.sec_title .item_title{color: #192049; font-size: 30px;}
.sec_title a{display: block; transition: .3s;}
.sec_title a:hover{opacity: 0.6;}
.sec_title a span{font-size: 12px; color: #192049; margin-right: 5px; font-weight: 600;}
.sec_title .count{display: inline; font-size: 45px; line-height: 1;}


.card_slick .slick-list{overflow-y: visible; padding-bottom: 45px;}
.p_card{padding: 0 30px;}
.p_card a{transition: .3s;}
.p_card a:hover{opacity: .7;}
.p_card .card_img{text-align: center; margin-bottom: 15px;}
.p_card .card_img img{display: inline; transition: .3s;}
.p_card .content{position: relative;}
.p_card .content .wrp_option_btn{position: absolute; top: 0; right: 0;}
.p_card .content .wrp_option_btn .wrp_drop{position: relative;}
.p_card .content .wrp_option_btn .wrp_drop ul{position: absolute; top: 30px; right: 0; display: none; z-index: 3; width: 150px; border: 1px solid #ccc;}
.p_card .content .wrp_option_btn .wrp_drop.active ul{display: block; font-size: 13px;}
.p_card .content .wrp_option_btn .wrp_drop.active ul .item_title{background-color: #ccc; padding: 5px 0; text-align: center; border-bottom: 1px solid #ccc;}
.p_card .content .wrp_option_btn .wrp_drop.active ul li{background-color: #fff; padding: 3px 10px;}
.p_card .content .wrp_option_btn .wrp_drop.active ul li a{color: #2EADCE;}
.p_card .content .wrp_option_btn .wrp_drop.active ul li a img{width: 15px; display: inline-block; margin-right: 5px;}
.p_card .content .wrp_option_btn .wrp_drop.active ul li a i{font-size: 11px; margin-right: 8px;}
.p_card .content .wrp_option_btn .wrp_drop.active ul li:first-of-type{padding-top: 10px;}
.p_card .content .wrp_option_btn .wrp_drop.active ul li:last-of-type{padding-bottom: 10px;}
.p_card .content .option_btn{display: inline-block; opacity: 0; color: #383838; position: relative; z-index: 5; cursor: pointer;}
.p_card .content:hover .option_btn{opacity: 1;}
.p_card .content .option_btn i{margin-right: 0px; font-size: 14px;}
.p_card .content .cat{display: inline-block; font-size: 10px; color: #D1BA8A; margin-bottom: 10px; padding-right: 10px;}
.p_card .content .title{display: block; font-size: 12px; margin-bottom: 10px; color: #383838; line-height: 2; font-weight: 600; overflow: hidden; display: -webkit-box; -webkit-box-orient: vertical; -webkit-line-clamp: 2;}
.p_card .content .name{font-size: 12px; color: #666666;}

.card_slick .slick-prev{left: 0;}
.card_slick .slick-next{right: 0;}
.card_slick .slick-prev::before,.card_slick .slick-next::before{font-family: "Font Awesome 5 Free"; font-weight: 900; color: #192049; font-size: 15px; border: solid 1px #192049; width: 30px; height: 30px; border-radius: 50%; display: block; line-height: 25px;}
.card_slick .slick-prev,.card_slick .slick-next{z-index: 10; width: 30px; height: 30px;}
.card_slick .slick-prev::before{content: '\f053';}
.card_slick .slick-next::before{content: '\f054';}
.slick-prev::before, .slick-next::before{opacity: 1;}

/* mv */
.mv_slick li{background-color: #EDE3D0;}
.mv_slick li a{display: block; position: relative; height: 450px; max-width: 1100px; margin: 0 auto; padding: 0 50px;}
.mv_slick .mv_title{position: absolute; top: 0; bottom: 0; left: 0; height: 250px; margin: auto 0; color: #192049; z-index: 2;}
.mv_slick .mv_img{position: absolute; top: 0; bottom: 0; right: 0; height: 360px; margin: auto 0;}
.mv_slick .mv_title .number{font-size: 30px; color: #192049; margin-bottom: 20px; letter-spacing: 5px;}
.mv_slick .mv_title .main{display: inline-block; font-size: 26px; background-color: #fff; padding: 7px 10px; letter-spacing: 8px; margin-bottom: 15px;}
.mv_slick .mv_title .sub{display: inline-block; font-size: 26px; background-color: #fff; padding: 7px 10px; letter-spacing: 5px;}
.mv_slick .slick-prev{left: 25px; z-index: 10; width: 35px; height: 35px; border: solid 1px #192049; border-radius: 50%;}
.mv_slick .slick-next{right: 25px; z-index: 10; width: 35px; height: 35px; border: solid 1px #192049; border-radius: 50%;}
.mv_slick .slick-dots{bottom: 25%; max-width: 1100px; margin: 0 auto; left: 0; right: 0; text-align: left;}
.mv_slick .slick-dots li{background-color: transparent;}
.mv_slick .slick-dots li button::before{font-size: 11px;}
.mv_slick .slick-prev::before,.mv_slick .slick-next::before{font-family: "Font Awesome 5 Free"; font-weight: 900; color: #192049; opacity: 1;}
.mv_slick .slick-prev::before{content: '\f053';}
.mv_slick .slick-next::before{content: '\f054';}

/* search */
#search{padding: 80px 0;}
#search .item_search{max-width: 1000px; margin: 0 auto;}
#search .item_title{font-size: 20px; text-align: center; margin-bottom: 60px;}
#search form{border: solid 1px #192049; display: flex;}
#search form input{width: 100%; border: 0px; padding: 10px 15px; padding-left: 20px;}
#search form input::placeholder{letter-spacing: 2px;}
#search form button{background-color: transparent; border: 0px; padding-right: 15px;}

.card_img{position: relative;}
.card_img .rank{position: absolute; top: 0; left: 0; background-color: #D1BA8A; color: #fff; padding: 0px 10px; z-index: 10;}
.card_img .rank span{font-size: 30px; display: inline;}
.card_img .rank .small{font-size: 10px;}

/* banner */
#banner{background-color: #D1BA8A; position: relative; height: 350px;}
#banner a{display: block;}
#banner .ban_img{position: absolute; top: 0; right: 0;}
#banner .wrp_content{position: absolute; top: 0; bottom: 0; left: 0; right: 0; margin: auto; max-width: 1350px; height: 240px; z-index: 2;}
#banner .sec_title .item_title{color: #fff;}
#banner .sec_title::before{border-top: solid 1px #fff;}
#banner .wrp_content .title{color: #192049; margin-bottom: 30px; font-weight: 600;}
#banner .wrp_content .title .sub{font-size: 20px; margin-bottom: 10px;}
#banner .wrp_content .title .main{font-size: 30px; letter-spacing: 4px;}
#banner .wrp_content .wrp_induction{max-width: 1000px;}
#banner .wrp_content .day{font-size: 22px; color: #fff;}
#banner .wrp_content .day .count{font-size: 30px; display: inline;}
#banner .wrp_content .wrp_induction .flex span .flex span{color: #fff;}
#banner .wrp_content .wrp_induction a span{font-size: 18px; letter-spacing: 2px;}

/*--- single ---*/
#main_content .section_inner{padding-top: 100px;}
#main_content .item_thumb{width: 49%; text-align: center;}
#main_content .wrp_content{width: 49%;}
#main_content .item_head{margin-bottom: 40px;}
#main_content .item_head .item img{margin-right: 10px;}
#main_content .item_cat span{color: #fff; background-color: #D1BA8A; padding: 5px 30px; font-size: 12px;}
#main_content .item_title{margin-bottom: 60px;}
#main_content .item_title .num{color: #D1BA8A; font-size: 20px; margin-bottom: 10px;}
#main_content .item_title .title{line-height: 2; font-size: 18px; margin-bottom: 30px; letter-spacing: 2px;}
#main_content .item_title .name{margin-bottom: 15px;}
#main_content .dum img{max-width: 400px;}
.single_text{border: solid 1px #D1BA8A; padding: 50px 50px;}
#bottom_sns .section_inner{padding-top: 0px; padding-bottom: 150px;}
#bottom_sns a{display: block; margin-right: 20px;}
#bottom_sns a img{max-width: 40px;}
.play_box .play_bar{margin-bottom: 30px;}
.play_box .play_bar .item_bar{width: 80%;}
.play_box .play_bar .time{width: 15%;}
.play_box .play_bar .time{font-size: 12px;}
.play_box .play_bar .time .orange{display: inline; color: #FF4804;}
.play_box .play_bar .item_bar input{accent-color: #F04F4F; width: 100%;}
.wrp-play_pase{margin-bottom: 40px;}
.wrp-play_pase .keep a{color: #383838;}
.wrp-play_pase .keep a img{margin-right: 5px;}
.wrp-play_pase .play_pase span{margin-right: 10px;}
.wrp-play_pase .play_pase .wrp_select select{background-color: #fff; border: solid 1px #383838; padding-left: 10px;}
.wrp-play_pase .play_pase .wrp_select select option{line-height: 1;}
.play_box .play_btn button{background-color: transparent; border: 0px;}
	


/*--- single-video ---*/
.container.v2{max-width: 1000px;}
#main_content.video_single .section_inner{padding-top: 50px;}
#main_content .video_content .item_head{margin-bottom: 40px;}
#main_content .video_content .item_head .item img{margin-right: 10px;}
#main_content .video_content .item_cat{margin-left: 20px;}
#main_content .video_content .item_cat span{color: #fff; background-color: #D1BA8A; padding: 5px 30px; font-size: 12px;}
#main_content .video_content .item_title{margin-bottom: 60px;}
#main_content .video_content .item_title .num{color: #D1BA8A; font-size: 20px; margin-bottom: 10px;}
#main_content .video_content .item_title .title{line-height: 2; font-size: 18px; margin-bottom: 30px; letter-spacing: 2px;}
#main_content .video_content .item_title .name{margin-bottom: 15px;}
.video_thumb{text-align: center; margin-bottom: 50px;}

/*--- watch ---*/
#big_title{margin-bottom: 20px;}
#big_title:first-of-type{margin-top: 100px;}
#big_title .title{font-size: 40px; font-weight: bold; color: #D1BA8A; text-align: center; letter-spacing: 10px; text-indent: 10px;}
#watch_now ul,#watch_was ul{margin-bottom: 70px;}


/*--- playlist ---*/
#playlist ul{margin-bottom: 70px;}
.add_list_box{text-align: center; padding-top: 70px; margin-bottom: 70px;}
.add_list_box a{display: inline-block; border: 1px solid #bebebe; padding: 7px 30px; text-align: center; color: #c0c0c0;}
.modal{display: none; height: 100vh; position: fixed; top: 0; width: 100%;}
.modal__bg{background: rgba(0,0,0,0.8); height: 100vh; position: absolute; width: 100%;}
.modal__content{background: #fff; left: 50%; padding: 40px; position: absolute; top: 50%; transform: translate(-50%,-50%); width: 500px;}
.modal__content .wrp_content{position: relative;}
.modal__content .wrp_content .js-modal-close{position: absolute; top: -50px; right: -50px; display: block; width: 25px; height: 25px; border-radius: 50%; background-color: #646464; text-align: center;}
.modal__content .wrp_content .js-modal-close i{font-size: 18px; color: #fff; margin-right: 0;}
.wrp_input input{border: 1px solid #bebebe; width: 100%; padding: 5px 10px;}
.modal__content .wrp_input{margin-bottom: 15px;}
.wrp_content .wrp_add{text-align: center;}
.wrp_content .wrp_add .add_btn{background-color: #646464; border-radius: 30px; color: #fff; display: inline-block; padding: 3px 60px;}

.grid .p_card{margin-bottom: 30px;}
.page_nation{text-align: center;}
.page_nation a{display: inline-block; color: #fff; background-color: #B2B2B2; padding: 3px 5px 4px; margin-right: 10px; font-size: 14px; line-height: 1;}
.page_nation .active{background-color: #192049;}
.page_nation .next{padding: 3px 12px;}


/*--- footer ---*/
.foot_inner{padding: 200px 0; background-color: #192049; text-align: center;}
.copy{color: #fff; font-size: 12px; padding: 12px 0 10px; border-top: solid 0.5px #fff; background-color: #0F132C; text-align: center; line-height: 1;}


/*------PC3------*/
