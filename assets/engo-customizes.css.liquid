  .container{max-width: 1140px;}
  #banner .wrp_content{max-width: 1140px;}
  #banner .wrp_content .wrp_induction{max-width: 750px;}
  
}
  
/*------PC2------*/
@media only screen and (max-width: 1440px) {

}


