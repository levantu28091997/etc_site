.prive_box .text .pdf_btn{border: 1px solid #D12832; color: #D12832; padding: 7px 20px; display: inline-block; margin-top: 5px; font-size: 14px;}

/* mypage */
.wrp_g_box{border: 1px solid #D1BA8A; padding: 50px 50px;}
.wrp_g_box .set_box{text-align: right;}
.wrp_g_box .set_box a{font-size: 13px; color: #000081;}
.wrp_g_box .info_box .name{color: #000;}
.product_box .pi_item{display: block; width: 18%; margin: 0 1%;}
.product_box .pi_item .item_img{margin-bottom: 10px; position: relative;}
.product_box .pi_item .item_img img,
.product_box .pa_item .item_img img
{width: 100%;}
.product_box .pi_item .item_img .cat,
.product_box .pa_item .item_img .cat
{position: absolute; bottom: 0; right: 0; display: inline-block; background-color: #D1BA8A; color: #fff; font-size: 12px; padding: 5px 10px; line-height: 1;}
.product_box .pi_item .item_tit{overflow: hidden; display: -webkit-box; -webkit-box-orient: vertical; -webkit-line-clamp: 2;}
.product_box .pa_item{margin-bottom: 15px; display: block;}
.product_box .pa_item .item_img{width: 15%; position: relative;}
.product_box .pa_item .item_tit{width: 85%; padding-left: 30px; padding-top: 10px;}
.product_box .pa_item .item_tit .main{text-decoration: underline; font-size: 18px; margin-bottom: 10px; color: #333333;}
.product_box .pa_item .item_tit .day{color: #D12832;}

/* order-history */
.history_box .t_item{width: 12%; margin: 0 1.1%; padding-bottom: 15px; text-align: center; font-size: 14px;}
.history_box .title_box .t_item{border-bottom: 2px solid #D1BA8A;}
.history_box .content_box a{display: block;}
.history_box .content_box .t_item{border-bottom: 1px solid #D1BA8A; display: flex; align-items: center; padding-bottom: 10px; padding-top: 10px;}
.history_box .content_box .t_item > a,
.history_box .content_box .t_item > span
{width: 100%;}
.history_box .content_box .t_item > a{color: #000081;}
.history_box .content_box .t_item.receipt{line-height: 1.2;}
.history_box .content_box .t_item.receipt img{width: 20px;}
.b_page_count{max-width: 1000px; margin: 20px auto 30px;}
.b_page_count .count{color: #666666; font-size: 14px; margin-right: 10px;}
.b_page_count .over_select select{background-color: #fff; font-size: 14px; border: solid 1px #666666; padding: 0px 5px;}

/* order-detail */
.ebtn_box .gold_btn .dw_icon{width: 20px; margin-right: 10px; padding-bottom: 5px;}
.oh_detail_box .box_title{padding-bottom: 20px; border-bottom: 2px solid #D1BA8A; margin-bottom: 20px;}
.oh_detail_box .box_product{padding-bottom: 20px; margin-bottom: 20px; border-bottom: 1px solid #D1BA8A;}
.oh_detail_box .box_product .p_img{width: 15%;}
.oh_detail_box .box_product .p_tit{width: 85%; padding-left: 20px;}
.oh_detail_box .box_price{margin-bottom: 80px;}
.oh_detail_box .box_price .item_price{margin-bottom: 10px;}
.oh_detail_box .box_price .sum_price{border-top: 1px solid #D1BA8A; margin-top: 20px; padding: 20px 10px 0;}
.oh_detail_box .box_price .sum_price span:nth-of-type(1){font-size: 18px;}
.oh_detail_box .box_address{padding-bottom: 20px; border-bottom: 1px solid #D1BA8A; margin-bottom: 20px;}
.oh_detail_box .box_address,.oh_detail_box .box_claim{position: relative;}
.oh_detail_box .box_address .status,.oh_detail_box .box_claim .status{position: absolute; top: 0; right: 0; display: inline-block; list-style: 1px; font-size: 12px; padding: 3px 10px;}
.oh_detail_box .box_address .status.gold,.oh_detail_box .box_claim .status.gold{background-color: #D1BA8A; color: #fff;}
.oh_detail_box .box_address .status.gray,.oh_detail_box .box_claim .status.gray{background-color: #CBCBCB; color: #000;}
.oh_detail_box .box_address .item_title,.oh_detail_box .box_claim .item_title{margin-bottom: 15px;}
.oh_detail_box .box_address .item_name,.oh_detail_box .box_claim .item_name{margin-bottom: 5px;}

